<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="127.0" prefWidth="342.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="crushers.controllers.setLabelController">
   <children>
      <TextField fx:id="labelField" layoutX="14.0" layoutY="64.0" promptText="Label" />
      <Label layoutX="14.0" layoutY="24.0" prefHeight="15.0" prefWidth="240.0" text="What is this transaction for?">
         <font>
            <Font name="SansSerif Regular" size="15.0" />
         </font>
      </Label>
      <Label fx:id="errorLabel" layoutX="14.0" layoutY="95.0" />
      <Button layoutX="200.0" layoutY="91.0" mnemonicParsing="false" onAction="#submitLabel" text="Submit" />
      <Button layoutX="276.0" layoutY="91.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
   </children>
</AnchorPane>
